{
  "compilerOptions": {
      "outDir": "./built/",
      "sourceMap": true,
      "strict": true,
      "noImplicitReturns": true,
      "noFallthroughCasesInSwitch": true,
      "allowUnreachableCode": false,
      "allowUnusedLabels": false,
      "strictNullChecks": false, 
      "module": "es2015",
      "moduleResolution": "node",
      "target": "es5",
      "allowJs": true,

  },
  "include": [
      "./src/**/*"
  ]
}